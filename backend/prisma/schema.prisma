
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Incident {
  id         String   @id @default(cuid())
  type       String
  severity   String
  riskScore  Int
  signals    Json
  explanation String  @db.Text
  createdAt  DateTime @default(now())
}
